<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".GyroscopeUncalibrated">

    <TextView
            android:text="X"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/gyroscopeUncalibratedDriftX"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintBottom_toTopOf="@+id/textView26"/>
    <TextView
            android:text="Y"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/gyroscopeUncalibratedDriftY"
            android:textSize="24sp"
            android:layout_marginRight="8dp" android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toTopOf="@+id/textView27"/>
    <TextView
            android:text="Z"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/gyroscopeUncalibratedDriftZ"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView27" android:layout_marginRight="8dp"
            android:layout_marginEnd="8dp"/>
    <TextView
            android:text="Rate of rotation (without drift compensation) around the 3 axis."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView17" android:textSize="36sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent" android:textStyle="bold"/>
    <TextView
            android:text="Gyroscope X:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:id="@+id/textView18" android:textSize="30sp"
            app:layout_constraintTop_toBottomOf="@+id/textView17" android:layout_marginTop="16dp"/>
    <TextView
            android:text="Gyroscope Y:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:id="@+id/textView19" android:textSize="30sp"
            app:layout_constraintTop_toBottomOf="@+id/textView18" android:layout_marginTop="16dp"/>
    <TextView
            android:text="Gyroscope Z:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView20" android:textSize="30sp"
            app:layout_constraintTop_toBottomOf="@+id/textView19" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"/>
    <TextView
            android:text="Drift X:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView25" android:textSize="30sp"
            app:layout_constraintTop_toBottomOf="@+id/textView20" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"/>
    <TextView
            android:text="Drift Y:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView26" android:textSize="30sp"
            app:layout_constraintTop_toBottomOf="@+id/textView25" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"/>
    <TextView
            android:text="Drift Z:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView27" android:textSize="30sp"
            app:layout_constraintTop_toBottomOf="@+id/textView26" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginTop="16dp"/>
    <TextView
            android:text="X"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/gyroscopeUncalibratedX"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView18"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"/>
    <TextView
            android:text="Y"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/gyroscopeUncalibratedY"
            android:textSize="24sp"
            android:layout_marginRight="8dp" android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView19"/>
    <TextView
            android:text="Z"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/gyroscopeUncalibratedZ"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/textView20" android:layout_marginRight="8dp"
            android:layout_marginEnd="8dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>